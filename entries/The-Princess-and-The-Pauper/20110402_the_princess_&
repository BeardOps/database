	margin-top: -1px;
	position: relative;
	left: 8px;
	width: 940px;
}

#access .menu-header, div.menu {
/*font-family:rockwell, "Courier New", Courier,  georgia, arial, helvetica;*/
	font-weight: bold;
	text-transform: uppercase;
	text-decoration: none;
	font-size: 14px;
}

